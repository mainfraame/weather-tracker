                case'year':
                case'yrs':
                case'yr':
                case'y':
                    return r * y;
                case'days':
                case'day':
                case'd':
                    return r * d;
                case'hours':
                case'hour':
                case'hrs':
                case'hr':
                case'h':
                    return r * h;
                case'minutes':
                case'minute':
                case'mins':
                case'min':
                case'm':
                    return r * m;
                case'seconds':
                case'second':
                case'secs':
                case'sec':
                case's':
                    return r * s;
                case'milliseconds':
                case'millisecond':
                case'msecs':
                case'msec':
                case'ms':
                    return r;
                default:
                    return;
            }
        }
    }
}

